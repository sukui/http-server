{
  "name": "zanphp/http-server",
  "repositories": {
    "0": {
      "type": "git",
      "url": "https://github.com/sukui/server-base.git"
    }
  },
  "require": {
    "php": ">=5.6",
    "zanphp/server-base": "dev-master",
    "zanphp/http-view": "dev-master",
    "zanphp/coroutine": "dev-master",
    "zanphp/container": "dev-master",
    "zanphp/nosql": "dev-master",
    "zanphp/utilities": "dev-master",
    "zanphp/http-foundation": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/routing": "dev-master",
    "zanphp/database": "dev-master",
    "zanphp/service-store": "dev-master",
    "zanphp/framework": "dev-master",
    "zanphp/worker-monitor": "dev-master"
  },
  "require-dev": {
    "zanphp/config": "dev-master",
    "zanphp/testing": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\HttpServer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanPHP\\HttpServer\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}